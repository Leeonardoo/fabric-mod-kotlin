{
  "schemaVersion": 1,
  "id": "manhuntkt",
  "version": "${version}",

  "name": "Manhunt Kotlin",
  "description": "Dream's manhunt, built on Kotlin for the Fabric Loader, heavily based on ytg1234's version.",
  "authors": [
    "Leeonardoo"
  ],
  "contact": {
    "sources": "https://github.com/Leeonardoo/manhunt-fabric-kotlin"
  },

  "license": "LGPL-3.0-or-later",
  "icon": "assets/manhunt/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "com.github.leeonardoo.manhunt.ExampleModKt::init"
    ]
  },
  "mixins": [
    "manhunt.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.8.7",
    "fabric": "*",
    "fabric-language-kotlin": "*",
    "minecraft": "1.16.x"
